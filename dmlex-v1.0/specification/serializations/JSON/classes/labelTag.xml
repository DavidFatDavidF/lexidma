        <listitem>
            <para><literal>"tag"</literal>
                <glossterm>required</glossterm>, string</para>
        </listitem>
        <listitem>
            <para><literal>"typeTag"</literal>
                <glossterm>optional</glossterm>, string</para>
        </listitem>
        <listitem>
            <para><literal>"forHeadwords"</literal>
                <glossterm>optional</glossterm>, boolean</para>
        </listitem>
        <listitem>
            <para><literal>"forTranslations"</literal>
                <glossterm>optional</glossterm>, boolean</para>
        </listitem>        
        <listitem>
            <para><literal>"forCollocates"</literal>
                <glossterm>optional</glossterm>, boolean</para>
        </listitem>        
        <listitem>
            <para><literal>"description"</literal>
                <glossterm>optional</glossterm>, string</para>
        </listitem>
        <listitem>
            <para><literal>"forLanguages"</literal>
